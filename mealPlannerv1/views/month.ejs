<% include partials/monthHeader %>
<div class="page-header">
    <h1>My Month</h1>
</div>
<div class="container">
<div class="row text-center">
    <div class="col-xs-3">
        <h3>Recipes</h3>
        <% recipes.forEach(function(recipe) { %>
        <div class="draggable ui-widget-content" thisID="<%= recipe._id %>"><%= recipe.name %></div>
        <% });%>
        <button id="clearAll">Clear Calendar</button>
        <button id="saveButton">Save Calendar</button>
    </div>
    <div class="col-xs-9" id="calendar"></div>
    
</div>
</div>
</body>
</html>